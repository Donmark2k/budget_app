<%= form_with model: group, url: user_groups_path, local: true, multipart: true  do |form| %>  
  <div class="category_form">
    <h3 > Add New Category </h3>
      <div class="form_div">
      <%= form.label :name  %>
      <%= form.text_field :name, class:''%>
      <% form.object.errors.full_messages_for(:name).each do |message|%>
      <div><%= message %></div>
      <%end%>
    </div>
      <br><br>
     <div class="form_div">
    <%= form.label :icon, 'Chose icon:' %>
      <%= form.file_field :icon %>
        <% form.object.errors.full_messages_for(:icon).each do |message|%>
        <div><%= message %></div>
        <%end%>
     </div>
     <br>
      <%= form.submit "Create Category", class:"category_button"%>
    </div>
<%end%>